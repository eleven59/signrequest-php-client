{
    "name": "eleven59/signrequest-php-client",
    "version": "dev-master",
    "description": "Fork of Official PHP client for SignRequest to add Laravel 8+ support",
    "keywords": [],
    "homepage": "https://signrequest.com/",
    "license": "MIT",
    "authors": [
        {
            "name": "SignRequest",
            "homepage": "https://signrequest.com/"
        }
    ],
    "require": {
        "php": ">=8.0",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^7.0.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "squizlabs/php_codesniffer": "~2.6",
        "friendsofphp/php-cs-fixer": "~2.12"
    },
    "autoload": {
        "psr-4": { "SignRequest\\" : "src/" }
    },
    "autoload-dev": {
        "psr-4": { "SignRequest\\" : "test/" }
    }
}